{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "MailApp": "Host=localhost;Database=tailwind;Username=app_user;Password=use_env_var;Include Error Detail=true;SSL Mode=Require;Trust Server Certificate=true;"
  },
  "ContentDirectory": {
    
  },
  "GDPR": {
    "PrivacyPolicyVersion": "1.0",
    "PrivacyPolicyUrl": "https://localhost:5001/privacy",
    "DataRetentionPeriodInDays": 730,
    "SecurityContact": "privacy@tailwindtraders.dev"
  },
  "Security": {
    "AllowedOrigins": [
      "https://localhost:3000",
      "http://localhost:3000",
      "https://localhost:5001",
      "http://localhost:5000"
    ],
    "RequireHttps": false,
    "EnableApiKey": true
  }
}
