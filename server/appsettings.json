{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "MailApp": "REPLACE_WITH_ENVIRONMENT_VARIABLE"
  },
  "AllowedHosts": "*",
  "GDPR": {
    "PrivacyPolicyVersion": "1.0",
    "PrivacyPolicyUrl": "https://tailwindtraders.dev/privacy",
    "DataRetentionPeriodInDays": 730,
    "SecurityContact": "privacy@tailwindtraders.dev"
  },
  "Security": {
    "AllowedOrigins": [
      "https://tailwindtraders.dev",
      "https://www.tailwindtraders.dev"
    ],
    "RequireHttps": true,
    "EnableApiKey": true
  }
}
